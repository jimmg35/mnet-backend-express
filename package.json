{
  "name": "mnet-backend-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/service.ts --env=development",
    "prod": "ts-node src/service.ts --env=production",
    "compose:up:dev": "cd .devcontainer && docker compose --env-file ../envConfig/service-config/development.env -p mnet-devcontainer-backend up -d",
    "migrate:dev": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -n migration",
    "update:dev": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "seed:dev": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "migrate:prod": "cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -n migration",
    "update:prod": "cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "seed:prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "start:migrate:prod": "cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -n migration && cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:run && nodemon ./src/service.ts --env=production",
    "inspect": "typeorm-model-generator -h localhost -d postgres -p 5435 -u postgres -x user!QAZ@WSX -e postgres -o ./inspect-output",
    "build:docker": "docker build . -t mnet-backend-express"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/geojson": "^7946.0.10",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/multiparty": "^0.0.33",
    "@types/node": "^16.11.19",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.4",
    "@types/sha256": "^0.2.0",
    "@types/smtp-server": "^3.5.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "nodemon": "^2.0.20",
    "ts-node": "^10.8.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "command-line-args": "^5.2.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "fs-extra": "^10.0.0",
    "geojson": "^0.5.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multiparty": "^4.2.3",
    "node-fetch": "2.6.6",
    "nodemailer": "^6.7.2",
    "open": "^8.4.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sha256": "^0.2.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "yamljs": "^0.3.0"
  }
}